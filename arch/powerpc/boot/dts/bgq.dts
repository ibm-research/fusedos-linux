	model = "ibm,bluegeneq";
	compatible = "ibm,bluegeneq";

	memory {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x0>;
	};

	chosen {
		bootargs =
	"root=/dev/ram0 rdinit=/sbin/init raid=noautodetect loglevel=7";

		/*
		 * the BGQ wrapper locates a ramdisk and updates
		 * initrd-start/end
		 */
		linux,initrd-start = <0>;
		linux,initrd-end = <0>;
	};

