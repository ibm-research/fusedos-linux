                        d-cache-sets = <32>;
                        i-cache-size = <0x4000>;
                        i-cache-block-size = <64>;
                        i-cache-sets = <64>;
                        ibm,ppc-interrupt-server#s = <0 1 2 3>;
                        enable-method = "ibm,bgq-spintable";
